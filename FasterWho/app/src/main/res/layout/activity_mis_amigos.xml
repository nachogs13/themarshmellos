<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MisAmigos">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentMisAmigos"
        android:name="com.muei.apm.fasterwho.MisAmigosFragment"
        android:layout_width="391dp"
        android:layout_height="737dp"
        android:layout_marginTop="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_mis_amigos_list" />

    <RelativeLayout
        android:id="@+id/loadingPanel4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center" >

        <ProgressBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:indeterminate="true" />

    </RelativeLayout>


    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="@string/add_friend"
        app:backgroundTint="@color/blue_800"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <RelativeLayout
        android:id="@+id/sinRes4"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/textViewSinResultados4"
            android:layout_width="133dp"
            android:layout_height="26dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="90dp"
            android:ems="10"
            android:text="@string/sin_resultados"
            android:textColor="#000000"
            android:textSize="18sp"
            app:drawableTint="@color/black"
            android:visibility="gone"
            tools:ignore="RtlHardcoded,UseCompatTextViewDrawableXml" />
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>